{
    "name": "Go Ground2Space",
    "description": "Redirect Go Playground to Go Play Space to activate syntax highlighting goodness and other nice features",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": { 
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "webRequest",
        "*://play.golang.org/*",
        "*://*.golang.org/*",
        "webRequestBlocking"
    ]
}